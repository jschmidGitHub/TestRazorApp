@page
@model TestRazorApp.Pages.ProductModel
@{
	ViewData["Title"] = "Product Details";
}

<table>
<thead>Products:</thead>
<ul>
    @foreach (var product in Model.Products)
    {
        <tr>
            <td> @product.Name </td>
        </tr>
    }
</ul>
</table>

<br>    
<p>
    <a asp-page="Index">Back to Customer Selection</a>
</p>